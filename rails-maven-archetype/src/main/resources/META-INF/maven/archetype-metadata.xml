<?xml version="1.0" encoding="UTF-8"?><archetype-descriptor name="rails2 archetype">
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/webapp</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>public</directory>
      <includes>
        <include>**/*.html</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>lib/tasks</directory>
      <includes>
        <include>**/*.rake</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>config/initializers</directory>
      <includes>
        <include>**/*.rb</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>.gitignore</include>
        <include>README.maven</include>
        <include>Gemfile.maven</include>
      </includes>
    </fileSet>
  </fileSets>
  <requiredProperties>
    <requiredProperty key="database">
      <defaultValue>sqlite3</defaultValue>
    </requiredProperty>
    <requiredProperty key="railsVersion">
      <defaultValue>2.3.5</defaultValue>
    </requiredProperty>
  </requiredProperties>
</archetype-descriptor>
